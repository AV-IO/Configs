{
    "workbench.iconTheme": "vscode-icons",
    "files.autoSave": "onFocusChange",
    "editor.insertSpaces": false,
    "editor.wordWrap": "on",
    "editor.fontLigatures": true,
    "editor.fontFamily": "Fira Code",
    "telemetry.enableTelemetry": false,
    "git.allowForcePush": true,
    "go.formatTool": "gofmt",
    "workbench.colorTheme": "Kary Pro Colors Ôºç Dark",
    "todo-tree.tags": [
        "ToDo",
        "FixMe",
        "Bug",
        "Implement"
    ],
    "todo-tree.defaultHighlight": {
        "type": "tag",
        "opacity": 50
    },
    "todo-tree.customHighlight": {
        "ToDo": {
            "foreground": "green",
            "iconColour": "green",
            "icon": "check"
        },
        "FixMe": {
            "foreground": "red",
            "iconColour": "red",
            "icon": "tools"
        },
        "Bug": {
            "foreground": "red",
            "iconColour": "red",
            "icon": "bug"
        },
        "Implement": {
            "foreground": "yellow",
            "iconColour": "yellow",
            "icon": "plus"
        },
    },
    "todo-tree.statusBar": "tags",
    "window.zoomLevel": 0
}
